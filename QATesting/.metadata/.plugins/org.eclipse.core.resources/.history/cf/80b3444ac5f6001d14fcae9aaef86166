package com.sgtesting.qatest1;

public class asg8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a[][]= {{ 1,2,3,},{4,5,6},{7,8,9}};
		int b[][]={ {110,20,30},{12,56,67},{67,89,68}};
		int c[][]= new int [3][3];
		for(int i=0;i<=a.length-1;i++)
		{
			for(int j=0;j<=b[0].length-1;j++)
			{
				if(i ==1 )
				{//if row is 1
					c[i][j]= a[i][j]-b[i][j];
				System.out.print(c[i][j]);
				}
			}
			System.out.println();
		}

		for(int k=1 ;k <c.length-1; k++)
		{
			for(int l=0;l<=c[0].length-1;l++) 
			{
				System.out.print(c[k][l]);
			}
		}
	}
}


