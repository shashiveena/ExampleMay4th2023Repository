package com.sgtesting.qatest1;

public class asg6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[]= {1,2,3};
		int b[]= {1,2,3,};
		int c[]= new int[3];
		for(int i= 0;i<=a.length-1;i++)
		{
		  c[i]= a[i]+b[i];
		  
		}
		
		for(int j=0; j<=c.length-1;j++)
		System.out.println(c[j]);

	}

}
