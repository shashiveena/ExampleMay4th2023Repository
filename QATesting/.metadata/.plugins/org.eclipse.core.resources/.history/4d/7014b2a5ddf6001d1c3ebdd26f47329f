package com.sgtesting.qatest1;

public class Diaganolelements {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[][]= {{1,2,3,},{3,4,5},{6,7,8}};


		for(int i=0;i<=a.length-1;i++)//row
		{

			for(int j=0;j<=a[0].length-1;j++)//colm
			{

				if(i==j)//compare i==j for diagonal
				{
					System.out.println(a[i][j]);
				}

			}
		}
		/////////////////////////////////////////////
		
		for(int i=a.length-1;i<=0; i--)//row
		{

			for(int j=0;j<=a[0].length-1;j++)//colm
			{

				//compare i==j for diagonal
				{
					System.out.println(a[i][j]);
				}

			}
		}
		
		/////////////////////////////////////////////

	}
}
